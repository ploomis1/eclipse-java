package main;

public class Tree {
	Tree hey;
	Tree next;
	Node nodes;
	int count;
	
	
	public Tree(int depth){
		this.nodes= new Node(depth);
		this.count=0;
	}
	public String getRoot(){
		
	return	this.nodes.var;
	}
	//public int getHeight(){
		
	//}
	public void addNode(String var){
		this.nodes.addNode(var);
		
	}
	public void getDepth(){
		
	}
	private int parent(int pos) {
		return pos / 2;
	}

	/*
	 * gets left child
	 */
	private int leftChild(int pos) {
		return (2 * pos);
	}

	/*
	 * gets right child
	 */
	private int rightChild(int pos) {
		return (2 * pos) + 1;
	}

}
